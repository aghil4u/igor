        <Button Content="Login As" VerticalAlignment="Top" Margin="10,44,10,0" Height="23" Click="btn_LoginAs"/>
        <Button Content="Create New user (proxy)" VerticalAlignment="Top" Margin="10,16,10,0" Height="23" Grid.Column="1" Click="btn_LognAsRandomUser"/>


    </Grid>
